{#
/**
 * @file
 * Theme override for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 * - breadcrumb_divider: Breadcrumb divider character
 */
#}
{% if breadcrumb %}
  {# Optionally, add the current node title or page title to the breadcrumb #}
  {% set breadcrumb = breadcrumb|merge([{
    'text': drupal_title(),
    'is_current': true
  }]) %}
  
  {{ include('bootstrap_barrio:breadcrumb',
    { breadcrumb: breadcrumb,
      breadcrumb_divider: breadcrumb_divider
    })
  }}
{% endif %}
