{
    "name": "pantheon-upstreams/drupal-composer-managed",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "https://github.com/upenn/penn403.git"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        }
    ],
    "require": {
        "pantheon-upstreams/upstream-configuration": "dev-main",
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-recommended": "^10",
        "pantheon-systems/drupal-integrations": "^10",
        "drush/drush": "^13",
        "upenn/penn403": "dev-d8",
        "drupal/simplesamlphp_auth": "^4.0",
        "drupal/group": "^3.2",
        "drupal/term_reference_tree": "^2.0",
        "drupal/smart_date": "^4.1",
        "drupal/gcontent_moderation": "3.0.x-dev@dev",
        "drupal/pathauto": "^1.13",
        "drupal/admin_toolbar": "^3.5",
        "drupal/unpublished_node_permissions": "^1.5",
        "drupal/devel": "^5.3",
        "drupal/color_field": "^3.0",
        "drupal/easy_email": "^3.0",
        "drupal/symfony_mailer": "^1.5",
        "drupal/mailsystem": "^4.5",
        "drupal/bootstrap_layout_builder": "^2.1",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/layout_builder_blocks": "^1.1",
        "drupal/bootstrap5": "^4.0",
        "drupal/views_bootstrap": "^4.5",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/tb_megamenu": "^1.9",
        "drupal/group_relationship_tokens": "^1.0",
        "drupal/webform": "^6.2",
        "drupal/smtp": "^1.4",
        "drupal/group_permissions": "^2.0@alpha",
        "drupal/variationcache": "^1.5",
        "bacon/bacon-qr-code": "^2.0",
        "drupal/recreate_block_content": "^3.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/twig_tweak": "^3.4",
        "drupal/layout_builder_restrictions": "^3.0",
        "drupal/section_library": "^1.2",
        "drupal/fullcalendar_view": "^5.2",
        "drupal/views_fields_combine": "^10.0",
        "drupal/groupmedia": "^4.0",
        "drupal/multiple_fields_remove_button": "^2.2",
        "drupal/field_group": "^3.6",
        "drupal/svg_image_field": "^2.3",
        "drupal/entity_reference_revisions": "^1.12",
        "drupal/endroid_qr_code": "^4.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/layout_builder_modal": "^2.0",
        "drupal/layout_builder_tabs": "^1.0",
        "drupal/simplify_menu": "^3.2",
        "drupal/userprotect": "^1.3",
        "drupal/duration_field": "^2.1",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/redirect": "^1.10",
        "drupal/token_filter": "^2.2",
        "drupal/captcha": "^2.0",
        "drupal/recaptcha_v3": "^2.0",
        "drupal/element_class_formatter": "^2.1",
        "drupal/twig_attributes": "^2.0",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/views_data_export": "^1.5",
        "drupal/custom_view_filters": "^2.1",
        "drupal/verf": "^2.0",
        "drupal/metatag": "^2.1",
        "drupal/rename_admin_paths": "^3.0",
        "drupal/publication_date": "^3.0@RC",
        "drupal/fullcalendar": "^3.0",
        "drupal/calendar_view": "^2.1",
        "drupal/feeds": "^3.0",
        "drupal/seo_checklist": "^5.2",
        "drupal/views_aggregator": "^2.1",
        "drupal/complete_webform_exporter": "^1.0",
        "drupal/webform_views": "^5.5",
        "drupal/views_list_sort": "^1.5",
        "drupal/eca": "^2.1",
        "drupal/bpmn_io": "^2.0",
        "drupal/menu_per_role": "^1.8",
        "drupal/menu_item_role_access": "^2.4",
        "drupal/advagg": "^6.0@alpha"
    },
    "require-dev": {
        "drupal/core-dev": "^10"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": ["pantheon-systems/drupal-integrations"],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "enable-patching": true,
        "patches": {
            "drupal/endroid_qr_code": {
                "Endriod QR Code patch": "patches/endroid_qr_code.patch"
            },
            "drupal/duration_field": {
                "Field Attributes": "patches/duration_number_attributes.patch"
            },
            "drupal/custom_view_filters": {
                "Timestamp range date picker": "patches/date_range_picker.patch"
            },
            "drupal/core": {
                "getEntityTypeId": "https://www.drupal.org/files/issues/2024-06-26/3049332_d10.3.0.patch",
                "Views filter of timestamp type": "https://www.drupal.org/files/issues/2024-11-19/core_views-timestamp_date_filter-3204558-13.patch"
            },
            "drupal/simplesamlphp_auth": {
                "Get dynamically values and Updated in drupal": "patches/simple_auth.patch"
            }
        }
    },
    "autoload": {
        "classmap": ["upstream-configuration/scripts/ComposerScripts.php"]
    },
    "scripts": {
        "post-install-cmd": [
            "/bin/bash scripts/saml_config.sh"
        ],
        "pre-update-cmd": ["DrupalComposerManaged\\ComposerScripts::preUpdate"],
        "post-update-cmd": [
            "DrupalComposerManaged\\ComposerScripts::postUpdate",
            "/bin/bash scripts/saml_config.sh"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "platform": {
            "php": "8.2.0"
        },
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "simplesamlphp/composer-xmlprovider-installer": true,
            "simplesamlphp/composer-module-installer": true,
            "tbachert/spi": true,
            "endroid/installer": true,
            "cweagans/composer-patches": true
        }
    }
}
